{
    "name": "obisconcept/neos-bootstrap",
    "type": "neos-plugin",
    "authors": [
        {
            "name": "obis|CONCEPT GmbH & Co. KG",
            "email": "dev@obis-concept.de",
            "homepage": "http://www.obis-concept.de"
        }
    ],
    "support": {
        "email": "dev@obis-concept.de"
    },
    "description": "A Neos CMS package to integrate Twitter Bootstrap and Font Awesome. Besides it integrates some Bootstrap based node types.",
    "license": [
        "MIT"
    ],
    "require": {
        "neos/neos": "^4.0.0",
        "neos/nodetypes": "^4.0.0"
    },
    "suggest": {
        "obisconcept/neos-jquery": "Add this package, if you don't use obisconcept/neos-bootstrap package or if you don't inluded jQuery by yourself"
    },
    "autoload": {
        "psr-4": {
            "ObisConcept\\NeosBootstrap\\": "Classes"
        }
    },
    "extra": {
        "applied-flow-migrations": [
            "TYPO3.FLOW3-201201261636",
            "TYPO3.Fluid-201205031303",
            "TYPO3.FLOW3-201205292145",
            "TYPO3.FLOW3-201206271128",
            "TYPO3.FLOW3-201209201112",
            "TYPO3.Flow-201209251426",
            "TYPO3.Flow-201211151101",
            "TYPO3.Flow-201212051340",
            "TYPO3.TypoScript-130516234520",
            "TYPO3.TypoScript-130516235550"
        ]
    }
}